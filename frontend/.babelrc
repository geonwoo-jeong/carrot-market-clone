{
  "presets": [
    [
      "next/babel",
      {
        "transform-runtime": {
          "useESModules": false
        }
      }
    ]
  ],
  "plugins": [
    ["styled-components", { "ssr": true }],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./components",
          "@constants": "./constants",
          "@containers": "./containers",
          "@Icons": "./icons",
          "@pages": "./pages",
          "@store": "./store",
          "@utils": "./utils"
        }
      }
    ]
  ]
}
